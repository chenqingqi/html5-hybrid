/**
 * Model数据(封装数据独立模块,公开属性/方法/事件)
 */
module(function()
{
	
	/**
	 * 标题
	 */
	var _title = '公开课';
	
	function title(val)
	{
		if(!val)
		{
			return _title;
		}
		
		_title = val;
	}
	
	
	/**
	 * 内容
	 */
	var _content = '中国最大的职业教育平台';
	
	function content(val)
	{
		if(!val)
		{
			return _content;
		}
		
		_content = val;
	}
	
	
	/**
	 * 公开接口
	 */
	return{
		
		title:title,
		content:content
	}
})